<html>
	<head>
		<title>WormTraq</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
		<link rel="stylesheet" href="../css/main.css" />
		<!--[if lte IE 8]><link rel="stylesheet" href="assets/css/ie8.css" /><![endif]-->
        <script src="https://code.jquery.com/jquery-3.1.0.min.js" integrity="sha256-cCueBR6CsyA4/9szpPfrX3s49M9vUU5BgtiJj06wt/s=" crossorigin="anonymous"></script>
        <script type="text/javascript" src="https://rawgit.com/allensarkisyan/VideoFrame/master/VideoFrame.min.js"></script>

	</head>
	<body class="homepage">
		<div id="page-wrapper">

			<!-- Header -->
				<header id="header">
					<div class="logo container">
						<div>
							<h1><a id="logo">Analyze Video</h1>
						</div>
					</div>
                    
				</header>

			<!-- Nav -->
				<nav id="nav">
					<ul>
						<li><a href="/">Home</a></li>
						<li><a href="/analyze">Picture</a></li>
						<li><a href="/measure">Measure</a></li>
						<li class="current"><a href="/video">Video</a></li>
						<li><a href="/config">Config</a></li>
						<li><a href="/about">About</a></li>
					</ul>
				</nav>


			<!-- Main -->
				<div id="main-wrapper">
					<div id="main" class="container">
						<div class="row 200%">
							<div class="12u">
								<!-- Highlight -->
							    <section class="box highlight">
                                    
                                    <video height="30%" width="100%" id="video" controls> 
                                    <source src="<%= video_path %>"></source>
                                    </video>
                                    <br>
                                    <br>
                                    <h4>Process Image at Video Timestamp (May take a while)</h4>
                                    <hr>
                                    <form method="post" action="/video_analyze/<%= token %>">
                                        <div class="form-group">
                                        <label>Time at video (Seconds)</label>
                                        <input type="number" class="form-control" name="time" >
                                        <br><br>
                                        <label>Detection Threshold (%). See the Picture Analysis Tool for more information</label>
                                        <center><input type="text" style="width:15%" class="form-control" name="areapercent" ></center>
										<br><hr>
											<label><h4>Processing Settings</h4></label>
											<hr>
											<br>
											<label><h4>Contrast (1-100)</h4></label>
											<center><input type="number" class="form-control" name="contrast" value="65" style="width: 8em"/></center>
											<br>
											<label><h4>Hue (1-100)</h4></label>
											<center><input type="number" class="form-control" name="hue" value="100" style="width: 8em"/></center>
											<br>
											<label><h4>Vibrance (1-100)</h4></label>
											<center><input type="number" class="form-control" name="vibrance" value="100" style="width: 8em"/></center>
											<br>
											<label><h4>Noise Removal Cycles (Any positive integer)</h4></label>
											<center><input type="number" class="form-control" name="noise" value="20" style="width: 8em"/></center>
                                        </div><br>
                                        <button type="submit" class="btn btn-success" onclick="loading()">Process</button>
                                    </form>

                                    <br>
                                    <br>
                                    <h4>Get Velocity of Objects (May take a while)</h4>
                                    <hr>
                                    <form method="post" action="/video_velocity/<%= token %>">
                                        <div class="form-group">
                                        <label>Start Time at video (Seconds)</label>
                                        <input type="number" class="form-control" name="start_time" >
                                        <br><br>
										<label>End Time at video (Seconds)</label>
                                        <input type="number" class="form-control" name="end_time" >
                                        <br><br>
                                        <label>Detection Threshold (%). See the Picture Analysis Tool for more information</label>
                                        <center><input type="text" style="width:15%" class="form-control" name="areapercent_velocity" ></center>
                                        </div>
										<br>
										<hr>
											<label><h4>Processing Settings</h4></label>
											<hr>
											<br>
											<label><h4>Contrast (1-100)</h4></label>
											<center><input type="number" class="form-control" name="contrast_v" value="65" style="width: 8em"/></center>
											<br>
											<label><h4>Hue (1-100)</h4></label>
											<center><input type="number" class="form-control" name="hue_v" value="100" style="width: 8em"/></center>
											<br>
											<label><h4>Vibrance (1-100)</h4></label>
											<center><input type="number" class="form-control" name="vibrance_v" value="100" style="width: 8em"/></center>
											<br>
											<label><h4>Noise Removal Cycles (Any positive integer)</h4></label>
											<center><input type="number" class="form-control" name="noise_v" value="20" style="width: 8em"/></center><br>
                                        <button type="submit" class="btn btn-success" onclick="loading()">Process</button>
                                    </form>
									<h2 id="load_status" style="color: #ff0000"></h2>

                                    
									</section>
							</div>
						</div>
						
					</div>
				</div>

			<!-- Footer -->
				<footer id="footer" class="container">
						<div id="copyright">
							<ul class="menu">
								<li><a href="https://github.com/mike2151/WormTraq">Source Code</a></li><li><a href="/about">About</a<</li>
							</ul>
						</div>

				</footer>

			</div>

		<!-- Scripts -->
          <script>
			  function loading() {
				document.getElementById("load_status").innerHTML="LOADING...";
			  }
		 </script>
		

	</body>
</html>